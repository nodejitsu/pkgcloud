  "main":    "./lib/pkgcloud",
  "private": true,
  "scripts": { "test": "echo y  | ssh-keygen -f test/fixtures/testkey -q -t rsa -N '' 2>&1 > /dev/null && NOCK=on vows test/*/*/*-test.js --spec --isolate" },
